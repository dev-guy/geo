{
  "name": "geo-playwright-tests",
  "version": "1.0.0",
  "description": "Playwright tests for the Geo country selector",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "format": "prettier --write \"assets/**/*.js\" \"test/**/*.js\" \"*.js\"",
    "lint": "eslint \"assets/**/*.js\" \"test/**/*.js\" \"*.js\" --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "eslint": "^9.29.0",
    "prettier": "^3.6.0"
  }
}
