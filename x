-      >
-        {render_slot(@trigger)}
-      </div>
-
-      <div
-        id={@id && "#{@id}-mega-menu-content"}
-        phx-click-away={
-          @id &&
-            JS.remove_class("show-mega-menu",
-              to: "##{@id}-mega-menu-content",
