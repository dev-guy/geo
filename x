-      this.triggerWrapper?.firstElementChild || this.triggerWrapper;
-
-    if (!this.triggerEl) return;
-
-    // Setup accessibility attributes
-    if (!this.triggerEl.getAttribute("tabindex")) {
-      this.triggerEl.setAttribute("tabindex", "0");
