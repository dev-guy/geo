-        padding_size(@padding),
-        rounded_size(@rounded),
-        width_size(@width),
-        border_class(@border, @variant),
-        top_gap(@top_gap),
-        space_class(@space),
-        size_class(@size),
-        @font_weight,
-        @class
-      ]}
